<?xml version="1.0" encoding="UTF-8"?>  
<application name="test-executor" xmlns="urn:com:ni:web:privilege">  
    <!-- Use xml:lang to define localized descriptions. -->  
    <description xml:lang="en">TestStand Executor</description>    
    <privileges>  
        <!-- Define a privilege that requires the users role. -->  
        <privilege id="ModifyResource">  
            <!-- Privileges can have multiple descriptions too. -->  
            <description xml:lang="en">Allows executing tests</description>    
            <!-- Default mappings for this privilege. -->  
            <role id="users"/>  
        </privilege>  
    </privileges>  
</application>  